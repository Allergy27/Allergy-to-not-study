{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "java_Debug",
      "type": "java",
      "request": "launch",
      "mainClass": "${fileDirnameBasename}.${fileBasenameNoExtension}",
      "console": "externalTerminal",
      "preLaunchTask": "javac",
    },
    {
      "name": "go_Debug",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${fileDirname}/build/${fileBasenameNoExtension}.exe",
      "env": {},
      "args": [],
      "console": "externalTerminal",
      "preLaunchTask": "GoBuild"
      // "showLog": true
    },
    {
      "name": "cpp_Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}\\build\\${fileBasenameNoExtension}.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "D:\\msy64\\mingw64\\bin",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "D:\\msy64\\mingw64\\bin\\gdb.exe",
      "setupCommands": [
        {
          "description": "为 gdb 启用整齐打印",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C/C++ 编译可执行文件"
    },
    {
      "name": "python_Debug",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "rust_Debug",
      "type": "lldb",
      "request": "launch",
      "program": "${fileDirname}/../../target/debug/${fileBasenameNoExtension}",
      "cwd": "${fileDirname}/..",
      "console": "externalTerminal",
      "preLaunchTask": "rust_Build"
    }
  ]
}